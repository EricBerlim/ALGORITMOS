algoritmo "FUNÇÃO FIBONACCI"

var
  I, T1, T2, T3: INTEIRO
FUNCAO PROXIMOFIBONACCI(VAR N1, N2: INTEIRO):INTEIRO //REFERÊNCIA
Var
   N3: INTEIRO
INICIO
    N3 <- N1 + N2
    N1 <- N2
    N2 <- N3
    RETORNE N3
FIMFUNCAO
inicio
   T1 <- 0
   ESCREVAL(T1)
   T2 <- 1
   ESCREVAL(T2)
   PARA I <- 3 ATE 5 FACA //ATÉ 5 NÚMEROS
   T3 <- PROXIMOFIBONACCI(T1, T2)
   ESCREVAL(T3)
   FimPara
fimalgoritmo
